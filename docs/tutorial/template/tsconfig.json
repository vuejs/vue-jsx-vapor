{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "preserve",
    "jsxImportSource": "vue-jsx-vapor",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "types": ["vite/client"],
    "allowJs": true,
    "checkJs": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "noEmit": true,
    "skipLibCheck": true
  }
}
