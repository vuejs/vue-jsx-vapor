{
  "name": "@vue-jsx-vapor/eslint",
  "type": "module",
  "version": "3.1.13",
  "description": "Vue JSX Vapor ESLint Plugin",
  "license": "MIT",
  "homepage": "https://github.com/vuejs/vue-jsx-vapor#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuejs/vue-jsx-vapor.git"
  },
  "bugs": {
    "url": "https://github.com/vuejs/vue-jsx-vapor/issues"
  },
  "keywords": [
    "vue",
    "jsx",
    "vapor",
    "eslint"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "jsx-vapor-dev": "./src/index.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "DEV=true tsdown",
    "release": "bumpp && npm publish",
    "test": "vitest"
  },
  "dependencies": {
    "@prettier/sync": "^0.6.1"
  },
  "devDependencies": {
    "@typescript-eslint/utils": "^8.53.1",
    "eslint-vitest-rule-tester": "^3.0.1"
  }
}
